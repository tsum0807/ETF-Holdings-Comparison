datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ETF {
  id          Int       @id @default(autoincrement())
  name        String
  ticker      String
  description String?
  expenseRatio Float
  holdings    Holding[] @relation("ETFHoldings")
}

model Holding {
  id            Int       @id @default(autoincrement())
  ticker        String
  name          String
  percentage    Float
  exchangeCode  String
  etfs          ETF[]     @relation("ETFHoldings")
}